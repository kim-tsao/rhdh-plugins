{
  "name": "@red-hat-developer-hub/backstage-plugin-dcm-common",
  "description": "Common functionalities for the dcm plugin",
  "version": "0.1.0",
  "backstage": {
    "pluginId": "dcm",
    "pluginPackages": [
      "@red-hat-developer-hub/backstage-plugin-dcm",
      "@red-hat-developer-hub/backstage-plugin-dcm-backend",
      "@red-hat-developer-hub/backstage-plugin-dcm-common"
    ],
    "role": "common-library"
  },
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-developer/rhdh-plugins",
    "directory": "workspaces/dcm/plugins/dcm-common"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
